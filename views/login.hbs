<div class = "log">

	<h3>Log In</h3> 

	{{#if message}}
		<div class="error">{{ message }}</div><br><br>
	{{/if}}

	<form method="POST" action="/login">
		<label class = "ul">username:</label><input name="username" type="text" autofocus><br>
		<label class = "ul">password:</label><input name="password" type="password"><br><br>
		<input type="submit" value="Log In">	
	</form>

</div>